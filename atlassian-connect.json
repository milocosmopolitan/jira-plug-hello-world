{
    "key": "progress-board-react",
    "name": "React Progress Board for Jira",
    "description": "This is just a tribute...",
    "vendor": {
        "name": "Milo Kang",
        "url": "milowebmaster@gmail.com"
    },
    "baseUrl": "{{localBaseUrl}}",
    "links": {
        "self": "{{localBaseUrl}}/atlassian-connect.json",
        "homepage": "{{localBaseUrl}}/atlassian-connect.json"
    },
    "authentication": {
        "type": "jwt"
    },
    "lifecycle": {
        "installed": "/installed"
    },
    "scopes": [
        "READ"
    ],
    "modules": {
        "jiraProjectPages": [
            {
                "key": "progress-board-sidebar",
                "name": {
                    "value": "Progress Board"
                },
                "url": "/progress-board?projectKey=${project.key}",
                "iconUrl": "/images/icon.svg",
                "weight": 1
            }
        ]
    }
}